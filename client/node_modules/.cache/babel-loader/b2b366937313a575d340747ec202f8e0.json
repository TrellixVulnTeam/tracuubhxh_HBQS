MAiBAC,QAjBA;MAkBAC,eAlBA;MAmBAC,WAnBA;MAoBAC;IApBA;EAsBA,CAxBA;;EAyBAC;IACA;EACA,CA3BA;;EA4BAC,YACA;MACAC;IADA,EADA;;IAKAC;MACA;MACA,wBACAf,MADA,CACAgB,eADA,EAEAC,GAFA,CAEAD;QACA;UAAAE;UAAAC;QAAA;MACA,CAJA;IAKA,CAZA;;IAaAC;MACA;IACA;;EAfA,CA5BA;EA6CAC;IACAC;MACA;IACA,CAHA;;IAIAC;MACA;IACA,CANA;;IAOAC;MACA;QACA;MACA;IACA,CAXA;;IAYAC;MACA;MACA;MACA;IACA,CAhBA;;IAiBAC;MACA;MACA;MACA,iBAHA,CAIA;IACA,CAtBA;;IAuBAC;MACA;MACA;IACA,CA1BA;;IA2BAC;MACA;MACA;QACAC,MADA;QAEAC,QAFA;QAGAC,cAHA;QAIAC,QAJA;QAKAC,SALA;QAMAC;MANA;MAQA;MACA;IACA,CAvCA;;IAwCAC;MACA;MACA;IACA,CA3CA;;IA4CAC;MACA;QACA;MACA;IACA;;EAhDA;AA7CA","names":["data","tablefields","key","label","sortable","filter","totalRows","currentPage","perPage","pageOptions","add","save","edit","item","selectedSku","index","selectMode","created","computed","items","sortOptions","f","map","text","value","selectedRows","methods","getIndex","deleteRow","rowSelected","onFiltered","onSave","doCancel","addRecord","id","name","transferId","date","total","actions","searchData","onEdit"],"sourceRoot":"src/components/reports","sources":["transfer-report.vue"],"sourcesContent":["<template>\n  <div class=\"col-sm-12\">\n    <px-card title=\"Transfer Report\">\n      <div class=\"card-body\">\n        <div class=\"table-responsive datatable-vue product-list\">\n          <b-modal id=\"modal-2\" title=\"Confirmation\" @ok=\"deleteRow(selectedSku)\">\n            <p class=\"my-4\">Are you sure!</p>\n          </b-modal>\n          <b-table\n            striped\n            hover\n            class=\"text-center\"\n            bordered\n            head-variant=\"light\"\n            show-empty\n            stacked=\"md\"\n            :items=\"items\"\n            :fields=\"tablefields\"\n            :filter=\"filter\"\n            :current-page=\"currentPage\"\n            :per-page=\"perPage\"\n            @filtered=\"onFiltered\"\n            :select-mode=\"selectMode\"\n            :selectable=\"true\"\n            @row-selected=\"rowSelected\"\n          >\n            <template v-slot:cell()=\"{ value, item, field: { key } }\">\n              <template v-if=\"edit != item.id\">{{ value }}</template>\n              <b-form-input type=\"text\" v-else v-model=\"item[key]\" />\n            </template>\n            <template v-slot:cell(actions)=\"{ item: { id } }\">\n              <span\n                style=\"font-size: 20px; color: blue;\"\n                v-if=\"edit != id && id !== null\"\n                @click=\"onEdit(id)\"\n              >\n                <feather\n                  type=\"edit-2\"\n                  stroke=\"#3758FD\"\n                  stroke-width=\"1\"\n                  size=\"18px\"\n                  fill=\"#3758FD\"\n                  stroke-linejoin=\"round\"\n                ></feather>\n              </span>\n              <span\n                v-if=\"edit != id\"\n                v-b-modal.modal-2\n                @click=\"getIndex(id)\"\n                style=\"font-size: 20px; color: #FA399F;\"\n              >\n                <feather\n                  type=\"trash\"\n                  stroke=\"#F72E9F\"\n                  size=\"18px\"\n                  fill=\"#F72E9F\"\n                ></feather>\n              </span>\n              <feather\n                v-if=\"edit === id && id !== ''\"\n                size=\"18px\"\n                stroke=\"#39CCC6\"\n                stroke-width=\"5\"\n                type=\"check\"\n                @click=\"onSave(id)\"\n              ></feather>\n              <feather\n                stroke=\"#F7299D\"\n                size=\"18px\"\n                stroke-width=\"5\"\n                v-if=\"edit === id && id !== ''\"\n                type=\"x\"\n                @click=\"doCancel(id)\"\n              ></feather>\n              <feather\n                stroke=\"#F7299D\"\n                size=\"18px\"\n                stroke-width=\"5\"\n                v-if=\"save && id == ''\"\n                type=\"save\"\n                @click=\"onSave(id)\"\n              ></feather>\n            </template>\n            <template v-slot:head(actions)>\n              <feather\n                v-if=\"!add\"\n                type=\"plus\"\n                stroke=\"#4050F6\"\n                stroke-width=\"6\"\n                size=\"22px\"\n                fill=\"#4050F6\"\n                @click=\"addRecord()\"\n              ></feather>\n              <feather\n                v-if=\"add\"\n                type=\"search\"\n                stroke-width=\"6\"\n                size=\"22px\"\n                @click=\"searchData()\"\n              ></feather>\n            </template>\n          </b-table>\n        </div>\n      </div>\n    </px-card>\n  </div>\n</template>\n\n<script>\nimport { mapGetters } from \"vuex\";\n\nexport default {\n  data() {\n    return {\n      tablefields: [\n        { key: \"name\", label: \"Name\", sortable: false },\n        { key: \"transferId\", label: \"Transfer Id\", sortable: false },\n        { key: \"date\", label: \"Date\", sortable: false },\n        { key: \"total\", label: \"Total\", sortable: false },\n        { key: \"actions\" }\n      ],\n\n      filter: null,\n      totalRows: 1,\n      currentPage: 1,\n      perPage: 10,\n      pageOptions: [5, 10, 15],\n      add: false,\n      save: false,\n      edit: null,\n      item: \"\",\n      selectedSku : \"\",\n      index: null,\n      selectMode: \"single\"\n    };\n  },\n  created() {\n    this.$store.dispatch(\"reports/getTransferData\");\n  },\n  computed: {\n    ...mapGetters({\n      items: \"reports/getTransferData\"\n    }),\n\n    sortOptions() {\n      // Create an options list from our fields\n      return this.tablefields\n        .filter(f => f.sortable)\n        .map(f => {\n          return { text: f.label, value: f.key };\n        });\n    },\n    selectedRows() {\n      return this.items.filter(item => item.delete);\n    }\n  },\n  methods: {\n    getIndex(id) {\n      this.selectedSku = id\n    },\n    deleteRow(id) {\n      this.items.splice(this.items.findIndex(e => e.id === id),1);\n    },\n    rowSelected(item) {\n      if (item[0]) {\n        this.edit = this.edit !== item[0].id ? item[0].id : null;\n      }\n    },\n    onFiltered(filteredItems) {\n      // Trigger pagination to update the number of buttons/pages due to filtering\n      this.totalRows = filteredItems.length;\n      this.currentPage = 1;\n    },\n    onSave(id) {\n      this.save = false;\n      this.add = false;\n      this.edit = 1000;\n      //  this.edit = ''\n    },\n    doCancel(id) {\n      this.edit = null;\n      this.item = null;\n    },\n    addRecord() {\n      this.add = true;\n      this.items.unshift({\n        id: \"\",\n        name: \"\",\n        transferId: \"\",\n        date: \"\",\n        total: \"\",\n        actions: \"\"\n      });\n      this.edit = \"\";\n      this.save = true;\n    },\n    searchData() {\n      this.add = false;\n      this.items.splice(0, 1);\n    },\n    onEdit(id) {\n      if (id !== \"undefined\") {\n        this.edit = this.edit !== id ? id : null;\n      }\n    }\n  }\n};\n</script>"]}]}