eather>\n        </li>\n        <li>\n          <feather\n            type=\"twitter\"\n            stroke=\"#F98085\"\n            size=\"16px\"\n            class=\"icon-twitter\"\n          ></feather>\n        </li>\n        <li>\n          <feather\n            type=\"instagram\"\n            stroke=\"#F98085\"\n            size=\"16px\"\n            class=\"icon-instagram\"\n          ></feather>\n        </li>\n        <li>\n          <feather\n            type=\"github\"\n            stroke=\"#F98085\"\n            size=\"16px\"\n            class=\"icon-instagram\"\n          ></feather>\n        </li>\n      </ul>\n    </div>\n  </form>\n</template>\n\n<script>\nimport Userauth from \"../../auth/index.js\";\nimport firebase from \"firebase\";\nexport default {\n  components: {},\n  data() {\n    return {\n      type: \"password\",\n      username: \"tuanpda.bhxh@gmail.com\",\n      password: \"123456\",\n      submitted: false\n    };\n  },\n  created() {\n    // reset login status for JWT\n    this.$store.dispatch(\"authentication/logout\");\n  },\n  methods: {\n    handleSubmit() {\n      this.submitted = true;\n    },\n    login() {\n      this.submitted = true;\n      if (this.email == \"\" && this.password == \"\") {\n        (this.email = \"tuanpda.bhxh@gmail.com\"), (this.password = \"123456\");\n      } else {\n        let data = {\n          username: this.username,\n          password: this.password\n        };\n\n        firebase\n          .auth()\n          .signInWithEmailAndPassword(data.username, data.password)\n          .then(\n            result => {\n              Userauth.localLogin(result);\n              if (result.user.refreshToken) {\n                localStorage.setItem(\n                  \"user\",\n                  JSON.stringify(result.user.refreshToken)\n                );\n              }\n            //  this.$toasted.show(\"succesfully loged in\", {\n            //    theme: \"bubble\",\n            //    position: \"top-right\",\n            //    type: \"success\",\n            //    duration: 2000\n            //  });\n              this.$router.push(\"/\");\n            },\n            err => {\n              this.username = \"tuanpda.bhxh@gmail.com\";\n              this.password = \"123456\";\n              this.$toasted.show(\"Oops...\" + err.message, {\n                theme: \"bubble\",\n                position: \"bottom-right\",\n                type: \"error\",\n                duration: 2000\n              });\n            }\n          );\n      }\n    }\n  }\n};\n</script>"]}]}