e.dispatch(\"auth/login3\", this.form).then(\n          () => {\n            this.$router.push(\"/dashboard-canbobaohiem/default\").catch(() => {});\n          },\n          (error) => {\n            this.message =\n              (error.response && error.response.data) ||\n              error.message ||\n              error.toString();\n          }\n        );\n      }\n    },\n  },\n};\n</script>"]}]}