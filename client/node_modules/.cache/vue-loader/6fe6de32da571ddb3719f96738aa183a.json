{"remainingRequest":"E:\\PROJECT\\TRACUUNO\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!E:\\PROJECT\\TRACUUNO\\src\\pages\\digital\\Category.vue?vue&type=style&index=0&id=16c2358e&lang=scss&scoped=true&","dependencies":[{"path":"E:\\PROJECT\\TRACUUNO\\src\\pages\\digital\\Category.vue","mtime":1625293629000},{"path":"E:\\PROJECT\\TRACUUNO\\node_modules\\css-loader\\dist\\cjs.js","mtime":499162500000},{"path":"E:\\PROJECT\\TRACUUNO\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":1655715156000},{"path":"E:\\PROJECT\\TRACUUNO\\node_modules\\postcss-loader\\src\\index.js","mtime":499162500000},{"path":"E:\\PROJECT\\TRACUUNO\\node_modules\\sass-loader\\dist\\cjs.js","mtime":499162500000},{"path":"E:\\PROJECT\\TRACUUNO\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"E:\\PROJECT\\TRACUUNO\\node_modules\\vue-loader\\lib\\index.js","mtime":1655715099000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:CnRhYmxlLnRleHQtY2VudGVyICogewogIHRleHQtYWxpZ246IGNlbnRlcjsKfQo="},{"version":3,"sources":["Category.vue"],"names":[],"mappings":";AA2QA;AACA;AACA","file":"Category.vue","sourceRoot":"src/pages/digital","sourcesContent":["<template>\n  <div>\n    <div class=\"container-fluid\">\n      <div class=\"page-header\">\n        <Breadcrumbs main=\"Digital\" title=\"Category\" />\n      </div>\n      <div class=\"row\">\n        <div class=\"col-md-12\">\n          <div class=\"card\">\n            <div class=\"card-header\">\n              <h5>Digital Product</h5>\n            </div>\n            <div class=\"card-body\">\n              <GlobalCategoryModal\n                @formData=\"formData\"\n                categoryType=\"digital\"\n              />\n              <div class=\"table-responsive datatable-vue product-physical\">\n                <b-modal id=\"modal-2\" title=\"Confirmation\" @ok=\"deleteRow(selectedSku)\">\n                  <p class=\"my-4\">Are you sure!</p>\n                </b-modal>\n                <b-table\n                  :select-mode=\"selectMode\"\n                  class=\"text-center\"\n                  striped\n                  hover\n                  head-variant=\"light\"\n                  bordered\n                  show-empty\n                  stacked=\"md\"\n                  :items=\"items\"\n                  :fields=\"tablefields\"\n                  :filter=\"filter\"\n                  :current-page=\"currentPage\"\n                  :per-page=\"perPage\"\n                  @filtered=\"onFiltered\"\n                  :selectable=\"true\"\n                  @row-selected=\"rowSelected\"\n                >\n                  <template v-slot:head(actions)>\n                    <feather\n                      type=\"plus\"\n                      stroke=\"#3758FD\"\n                      stroke-width=\"6\"\n                      size=\"22px\"\n                      fill=\"#3758FD\"\n                    ></feather>\n                  </template>\n                  <template #cell(status)=\"field\">\n                    <statusIcon :field=\"field\"></statusIcon>\n                  </template>\n                  <template #cell(image)=\"field\">\n                    <img\n                      height=\"50px\"\n                      :src=\"getImgUrl(field.item.images)\"\n                      width=\"50px\"\n                    />\n                  </template>\n                  <template v-slot:cell()=\"{ value, item, field: { key } }\">\n                    <template v-if=\"edit != item.id\">{{ value }}</template>\n                    <b-form-input type=\"text\" v-else v-model=\"item[key]\" />\n                  </template>\n                  <template v-slot:cell(actions)=\"{ item: { id } }\">\n                    <span\n                      style=\"font-size: 20px; color: blue;\"\n                      v-if=\"edit != id && id !== null\"\n                      @click=\"onEdit(id)\"\n                    >\n                      <feather\n                        type=\"edit-2\"\n                        stroke=\"#3758FD\"\n                        stroke-width=\"1\"\n                        size=\"18px\"\n                        fill=\"#3758FD\"\n                        stroke-linejoin=\"round\"\n                      ></feather>\n                    </span>\n                    <span\n                      v-if=\"edit != id\"\n                      v-b-modal.modal-2\n                      @click=\"getIndex(id)\"\n                      style=\"font-size: 20px; color: #FA399F;\"\n                    >\n                      <feather\n                        type=\"trash\"\n                        stroke=\"#F72E9F\"\n                        size=\"18px\"\n                        fill=\"#F72E9F\"\n                      ></feather>\n                    </span>\n                    <feather\n                      v-if=\"edit === id && id !== ''\"\n                      size=\"18px\"\n                      stroke=\"#39CCC6\"\n                      stroke-width=\"5\"\n                      type=\"check\"\n                      @click=\"onSave(id)\"\n                    ></feather>\n\n                    <feather\n                      stroke=\"#F7299D\"\n                      size=\"18px\"\n                      stroke-width=\"5\"\n                      v-if=\"edit === id && id !== ''\"\n                      type=\"x\"\n                      @click=\"doCancel(id)\"\n                    ></feather>\n                    <feather\n                      stroke=\"#F7299D\"\n                      size=\"18px\"\n                      stroke-width=\"5\"\n                      v-if=\"save && id == ''\"\n                      type=\"save\"\n                      @click=\"onSave(id)\"\n                    ></feather>\n                  </template>\n                  <template v-slot:head(actions)>\n                    <feather\n                      v-if=\"!add\"\n                      type=\"plus\"\n                      stroke=\"#4050F6\"\n                      stroke-width=\"6\"\n                      size=\"22px\"\n                      fill=\"#4050F6\"\n                      @click=\"addRecord()\"\n                    ></feather>\n                    <feather\n                      v-if=\"add\"\n                      type=\"search\"\n                      stroke-width=\"6\"\n                      size=\"22px\"\n                      @click=\"searchData()\"\n                    ></feather>\n                  </template>\n                </b-table>\n              </div>\n              <b-col md=\"12\" class=\"my-1 pagination-justify p-0\">\n                <b-pagination\n                  v-model=\"currentPage\"\n                  :total-rows=\"totalRows\"\n                  :per-page=\"perPage\"\n                  aria-controls=\"my-table\"\n                  class=\"mt-4\"\n                ></b-pagination>\n              </b-col>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- Container-fluid Ends-->\n  </div>\n</template>\n\n<script>\nimport { mapGetters } from \"vuex\";\nimport statusIcon from \"../../components/featherIcons/status-icon.vue\";\n\nexport default {\n  components: { statusIcon },\n  data() {\n    return {\n      tablefields: [\n        { key: \"image\", label: \"image\", sortable: false },\n        { key: \"name\", label: \"Name\", sortable: false },\n        { key: \"price\", label: \"Price\", sortable: false },\n        { key: \"status\", label: \"Status\", sortable: false },\n        { key: \"category\", label: \"Category\", sortable: false },\n        { key: \"actions\" }\n      ],\n      selectMode: \"single\",\n      filter: null,\n      totalRows: 1,\n      currentPage: 1,\n      perPage: 10,\n      pageOptions: [5, 10, 15],\n      add: false,\n      save: false,\n      edit: null,\n      item: \"\",\n      selectedSku : \"\",\n      index: null\n    };\n  },\n  created() {\n    this.$store.dispatch(\"digitalProducts/getProducts\");\n  },\n  computed: {\n    ...mapGetters({\n      items: \"digitalProducts/getProducts\"\n    }),\n    sortOptions() {\n      // Create an options list from our fields\n      return this.tablefields\n        .filter(f => f.sortable)\n        .map(f => {\n          return { text: f.label, value: f.key };\n        });\n    },\n    selectedRows() {\n      return this.items.filter(item => item.delete);\n    }\n  },\n  mounted() {\n    // Set the initial number of items\n    this.totalRows = this.items.length;\n  },\n  methods: {\n    getImgUrl(path) {\n      return require(\"@/assets/images/digital-product/\" + path);\n    },\n    getIndex(id) {\n      this.selectedSku = id\n    },\n    formData(e) {\n      console.log(\"modal data\", e);\n    },\n    onFiltered(filteredItems) {\n      // Trigger pagination to update the number of buttons/pages due to filtering\n      this.totalRows = filteredItems.length;\n      this.currentPage = 1;\n    },\n    onEdit(id) {\n      if (id !== \"undefined\") {\n        this.edit = this.edit !== id ? id : null;\n      }\n    },\n    onSave(id) {\n      this.save = false;\n      this.add = false;\n      this.edit = 1000;\n      //  this.edit = ''\n    },\n    doCancel(id) {\n      this.edit = null;\n      this.item = null;\n    },\n    deleteRow(id) {\n      this.items.splice(this.items.findIndex(e => e.id === id),1);\n    },\n    rowSelected(item) {\n      if (item[0]) {\n        this.edit = this.edit !== item[0].id ? item[0].id : null;\n      }\n    },\n    addRecord() {\n      this.add = true;\n      this.items.unshift({\n        id: \"\",\n        images: \"main.jpg\",\n        name: \"\",\n        price: \"\",\n        category: \"\",\n        actions: \"\"\n      });\n      this.edit = \"\";\n      this.save = true;\n    },\n    searchData() {\n      this.add = false;\n      this.items.splice(0, 1);\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\ntable.text-center * {\n  text-align: center;\n}\n</style>"]}]}